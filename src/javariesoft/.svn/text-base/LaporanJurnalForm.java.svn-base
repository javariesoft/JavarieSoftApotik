private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed
          HashMap parameter=new HashMap();
        JasperPrint jasperPrint=null;
        
        try { 
            if(cboPil.getSelectedIndex()==0){
                parameter.put("BULAN", cboBulan.getSelectedIndex()+1);
                parameter.put("TAHUN", txtTahun.getText());
                jasperPrint = JasperFillManager.fillReport("report\\jurnal.jasper", parameter,koneksi.getKoneksiJ());
            }else{
                parameter.put("tanggal", tgl.getText());
                jasperPrint = JasperFillManager.fillReport("report\\jurnalhari.jasper", parameter,koneksi.getKoneksiJ());
            }    
            JasperViewer.viewReport(jasperPrint,false);
        } catch (Exception ex) {
            System.out.print(ex.toString());
            //Logger.getLogger(formlaporan.class.getName()).log(Level.SEVERE, null, ex);
        }
    
}//GEN-LAST:event_jButton1ActionPerformed

