private void btnOkActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnOkActionPerformed
        try {
            // TODO add your handling code here:
                Statement stat=koneksi.getKoneksiJ().createStatement();
                stat.executeUpdate("update KODEAKSES set KODE='"+ txtKodeAkses.getText() +"'");
                stat.close();
                JOptionPane.showMessageDialog(null, "Entri Kode Ok, Silah kan Jalankan Program.");
        } catch (Exception ex) {
            Logger.getLogger(DialogRegister.class.getName()).log(Level.SEVERE, null, ex);
        }
    
}//GEN-LAST:event_btnOkActionPerformed

